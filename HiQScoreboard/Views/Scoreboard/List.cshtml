@model IEnumerable<HiQScoreboard.scoreboardresults>

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GetResultsView</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.office)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.q1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.q2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.q3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.q4)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.q5)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.office)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.q1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.q2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.q3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.q4)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.q5)
        </td>
        <td>
         
        </td>
    </tr>
}

</table>
